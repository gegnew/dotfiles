{
  "title": "germ chords",
  "rules": [
    {
      "description": "backspace",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "s"
              },
              {
                "key_code": "d"
              }
            ]
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ]
        }
      ]
    },
    {
      "description": "tab",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "d"
              },
              {
                "key_code": "f"
              }
            ]
          },
          "to": [
            {
              "key_code": "tab"
            }
          ]
        }
      ]
    },
    {
      "description": "return",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "k"
              },
              {
                "key_code": "l"
              }
            ]
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ]
        }
      ]
    },
    {
      "description": "escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "q"
              },
              {
                "key_code": "w"
              }
            ]
          },
          "to": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    },
    {
      "description": "escape 2",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "o"
              },
              {
                "key_code": "p"
              }
            ]
          },
          "to": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    },
    {
      "description": "dash",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "x"
              },
              {
                "key_code": "c"
              }
            ]
          },
          "to": [
            {
              "key_code": "hyphen"
            }
          ]
        }
      ]
    },
    {
      "description": "colon",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "j"
              },
              {
                "key_code": "k"
              }
            ]
          },
          "to": [
            {
              "key_code": "semicolon",
              "modifiers": [
                  "left_shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "backslash",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "p"
              },
              {
                "key_code": "open_bracket"
              }
            ]
          },
          "to": [
            {
              "key_code": "backslash"
            }
          ]
        }
      ]
    },
    {
      "description": "underscore",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "c"
              },
              {
                "key_code": "v"
              }
            ]
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": [
                  "left_shift"
              ]
            }
          ]
        }
      ]
    }
  ]
}
